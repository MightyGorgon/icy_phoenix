<table class="forumline" width="100%" cellspacing="0" cellpadding="0">
<tr>
	<td class="row2"><span class="genmed">{L_FROM}:</span></td>
	<td width="75%" class="row2" colspan="2"><span class="genmed">{FROM}{FROM_IP}</span></td>
</tr>
<tr>
	<td class="row2"><span class="genmed">{L_TO}:</span></td>
	<td class="row2" colspan="2"><span class="genmed">{TO}</span></td>
</tr>
<tr>
	<td class="row2"><span class="genmed">{L_SENT_DATE}:</span></td>
	<td class="row2" colspan="2"><span class="genmed">{DATE}</span></td>
</tr>
<tr>
	<td class="row2"><span class="genmed">{L_SUBJECT}:</span></td>
	<td class="row2"><span class="genmed">{SUBJECT}</span></td>
	<td nowrap="nowrap" class="row2" align="right"> &nbsp; </td>
</tr>
<tr><td valign="top" colspan="3" class="row1"><div class="post-text">{MESSAGE}</div></td></tr>
</table>
<br />